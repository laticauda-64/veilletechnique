{
	"extends": ["next", "next/core-web-vitals", "eslint:recommended"],
	"rules": {
		"no-unused-vars": ["warn"],
		"import/no-anonymous-default-export": [
			"error",
			{
				"allowObject": true
			}
		],
		"react/display-name": [0, { "ignoreTranspilerName": true }]
	},
	"env": {
		"browser": true,
		"node": true
	}
}
